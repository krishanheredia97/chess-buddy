{
  "manifest_version": 3,
  "name": "My First Extension",
  "version": "1.0.0",
  "description": "A simple Chrome extension built with TypeScript",
  "action": {
    "default_popup": "popup.html",
    "default_title": "My Extension"
  },
  "permissions": ["activeTab", "tabs", "scripting"],
  "host_permissions": [
    "https://*.chess.com/*",
    "http://*.chess.com/*"
  ]
}
